<use f='linux-4.14.y/drivers/gpu/drm/drm_atomic_helper.c' l='2187' u='r' c='drm_atomic_helper_disable_planes_on_crtc'/>
<use f='linux-4.14.y/drivers/gpu/drm/drm_atomic_helper.c' l='2188' u='r' c='drm_atomic_helper_disable_planes_on_crtc'/>
