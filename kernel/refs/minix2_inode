<mbr r='minix2_inode::i_gid' o='48' t='__u16'/>
<mbr r='minix2_inode::i_size' o='64' t='__u32'/>
<mbr r='minix2_inode::i_atime' o='96' t='__u32'/>
<mbr r='minix2_inode::i_mtime' o='128' t='__u32'/>
