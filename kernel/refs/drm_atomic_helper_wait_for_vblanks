<dec f='linux-4.14.y/drivers/gpu/drm/drm_atomic_helper.c' l='1245' type='void drm_atomic_helper_wait_for_vblanks(struct drm_device * , struct drm_atomic_state * )'/>
<use f='linux-4.14.y/drivers/gpu/drm/drm_atomic_helper.c' l='1245' c='drm_atomic_helper_wait_for_vblanks'/>
<use f='linux-4.14.y/drivers/gpu/drm/drm_atomic_helper.c' l='1245' u='a'/>
