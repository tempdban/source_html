<mbr r='xol_area::bitmap' o='256' t='unsigned long *'/>
<mbr r='xol_area::xol_mapping' o='320' t='struct vm_special_mapping'/>
<mbr r='xol_area::pages' o='576' t='struct page *[2]'/>
<mbr r='xol_area::vaddr' o='704' t='unsigned long'/>
