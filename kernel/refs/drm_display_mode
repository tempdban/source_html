<mbr r='drm_display_mode::crtc_hblank_start' o='1216' t='int'/>
<mbr r='drm_display_mode::crtc_hblank_end' o='1248' t='int'/>
<mbr r='drm_display_mode::crtc_hsync_start' o='1280' t='int'/>
<mbr r='drm_display_mode::crtc_hsync_end' o='1312' t='int'/>
<mbr r='drm_display_mode::crtc_htotal' o='1344' t='int'/>
<mbr r='drm_display_mode::crtc_hskew' o='1376' t='int'/>
<mbr r='drm_display_mode::crtc_vdisplay' o='1408' t='int'/>
<mbr r='drm_display_mode::crtc_vblank_start' o='1440' t='int'/>
<mbr r='drm_display_mode::crtc_vblank_end' o='1472' t='int'/>
<mbr r='drm_display_mode::crtc_vsync_start' o='1504' t='int'/>
<mbr r='drm_display_mode::crtc_vsync_end' o='1536' t='int'/>
<mbr r='drm_display_mode::crtc_vtotal' o='1568' t='int'/>
