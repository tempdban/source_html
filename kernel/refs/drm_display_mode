<mbr r='drm_display_mode::hskew' o='864' t='int'/>
<mbr r='drm_display_mode::vdisplay' o='896' t='int'/>
<mbr r='drm_display_mode::vsync_start' o='928' t='int'/>
<mbr r='drm_display_mode::vsync_end' o='960' t='int'/>
<mbr r='drm_display_mode::vtotal' o='992' t='int'/>
<mbr r='drm_display_mode::vscan' o='1024' t='int'/>
<mbr r='drm_display_mode::flags' o='1056' t='unsigned int'/>
