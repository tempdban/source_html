<def f='linux-4.14.y/include/linux/mmzone.h' l='840' ll='843' type='bool managed_zone(struct zone * zone)'/>
<doc f='linux-4.14.y/include/linux/mmzone.h' l='834'>/*
 * Returns true if a zone has pages managed by the buddy allocator.
 * All the reclaim decisions have to use this function rather than
 * populated_zone(). If the whole zone is reserved then we can easily
 * end up with populated_zone() &amp;&amp; !managed_zone().
 */</doc>
<use f='linux-4.14.y/mm/page_alloc.c' l='4880' u='c' c='build_zonerefs_node'/>
<use f='linux-4.14.y/mm/vmscan.c' l='259' u='c' c='lruvec_lru_size'/>
<use f='linux-4.14.y/mm/vmscan.c' l='2258' u='c' c='get_scan_count'/>
<use f='linux-4.14.y/mm/vmscan.c' l='2580' u='c' c='should_continue_reclaim'/>
<use f='linux-4.14.y/mm/vmscan.c' l='2932' u='c' c='allow_direct_reclaim'/>
<use f='linux-4.14.y/mm/vmscan.c' l='3205' u='c' c='pgdat_balanced'/>
<use f='linux-4.14.y/mm/vmscan.c' l='3286' u='c' c='kswapd_shrink_node'/>
<use f='linux-4.14.y/mm/vmscan.c' l='3359' u='c' c='balance_pgdat'/>
<use f='linux-4.14.y/mm/vmscan.c' l='3640' u='c' c='wakeup_kswapd'/>
<def f='linux-4.14.y/include/linux/mmzone.h' l='840' ll='843' type='bool managed_zone(struct zone * zone)'/>
