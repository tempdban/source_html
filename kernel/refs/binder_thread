<mbr r='binder_thread::todo' o='576' t='struct list_head'/>
<mbr r='binder_thread::process_todo' o='704' t='bool'/>
<mbr r='binder_thread::return_error' o='768' t='struct binder_error'/>
<mbr r='binder_thread::reply_error' o='1024' t='struct binder_error'/>
