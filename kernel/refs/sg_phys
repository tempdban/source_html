<use f='linux-4.14.y/drivers/iommu/amd_iommu.c' l='2513' u='c' c='map_sg'/>
<use f='linux-4.14.y/drivers/iommu/amd_iommu.c' l='2520' u='c' c='map_sg'/>
<use f='linux-4.14.y/drivers/iommu/amd_iommu.c' l='2542' u='c' c='map_sg'/>
<use f='linux-4.14.y/drivers/iommu/intel-iommu.c' l='2261' u='c' c='__domain_mapping'/>
