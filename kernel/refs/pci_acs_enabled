<dec f='linux-4.14.y/include/linux/pci.h' l='2070' type='bool pci_acs_enabled(struct pci_dev * pdev, u16 acs_flags)'/>
<use f='linux-4.14.y/drivers/iommu/iommu.c' l='831' u='c' c='get_pci_function_alias_group'/>
