<doc f='linux-4.14.y/include/linux/cgroup-defs.h' l='163'>/*
 * A css_set is a structure holding pointers to a set of
 * cgroup_subsys_state objects. This saves space in the task struct
 * object and speeds up fork()/exit(), since a single inc/dec and a
 * list_add()/del() can bump the reference count on the entire cgroup
 * set for a task.
 */</doc>
<mbr r='css_set::subsys' o='0' t='struct cgroup_subsys_state *[6]'/>
<mbr r='css_set::refcount' o='384' t='refcount_t'/>
<mbr r='css_set::dom_cset' o='448' t='struct css_set *'/>
<mbr r='css_set::dfl_cgrp' o='512' t='struct cgroup *'/>
<mbr r='css_set::nr_tasks' o='576' t='int'/>
<mbr r='css_set::tasks' o='640' t='struct list_head'/>
<mbr r='css_set::mg_tasks' o='768' t='struct list_head'/>
<mbr r='css_set::task_iters' o='896' t='struct list_head'/>
<mbr r='css_set::e_cset_node' o='1024' t='struct list_head [6]'/>
<mbr r='css_set::threaded_csets' o='1792' t='struct list_head'/>
<mbr r='css_set::threaded_csets_node' o='1920' t='struct list_head'/>
