<offset>0</offset>
<doc f='linux-4.14.y/include/linux/mmzone.h' l='223'>/*
	 * The pageout code in vmscan.c keeps track of how many of the
	 * mem/swap backed and file backed pages are referenced.
	 * The higher the rotated/scanned ratio, the more valuable
	 * that cache is.
	 *
	 * The anon LRU stats live in [0], file LRU stats in [1]
	 */</doc>
<use f='linux-4.14.y/mm/swap.c' l='270' u='w' c='update_page_reclaim_stat'/>
<use f='linux-4.14.y/mm/vmscan.c' l='1719' u='w' c='putback_inactive_pages'/>
<use f='linux-4.14.y/mm/vmscan.c' l='2072' u='w' c='shrink_active_list'/>
<use f='linux-4.14.y/mm/vmscan.c' l='2323' u='w' c='get_scan_count'/>
<use f='linux-4.14.y/mm/vmscan.c' l='2328' u='w' c='get_scan_count'/>
<use f='linux-4.14.y/mm/vmscan.c' l='2337' u='r' c='get_scan_count'/>
<use f='linux-4.14.y/mm/vmscan.c' l='2340' u='r' c='get_scan_count'/>
