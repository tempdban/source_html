<def f='linux-4.14.y/include/linux/netfilter/x_tables.h' l='360' ll='379' type='unsigned int xt_write_recseq_begin()'/>
<use f='linux-4.14.y/net/ipv4/netfilter/arp_tables.c' l='204' u='c' c='arpt_do_table'/>
