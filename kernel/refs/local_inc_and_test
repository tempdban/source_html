<doc f='linux-4.14.y/arch/x86/include/asm/local.h' l='72'>/**
 * local_inc_and_test - increment and test
 * @l: pointer to type local_t
 *
 * Atomically increments @l by 1
 * and returns true if the result is zero, or false for all
