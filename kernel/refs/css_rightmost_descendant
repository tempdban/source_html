<use f='linux-4.14.y/kernel/cgroup/cpuset.c' l='921' u='c' c='update_cpumasks_hier'/>
<use f='linux-4.14.y/kernel/cgroup/cpuset.c' l='1174' u='c' c='update_nodemasks_hier'/>
<dec f='linux-4.14.y/include/linux/cgroup.h' l='134' type='struct cgroup_subsys_state * css_rightmost_descendant(struct cgroup_subsys_state * pos)'/>
