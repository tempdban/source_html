<dec f='linux-4.14.y/drivers/gpu/drm/drm_atomic.c' l='1497' type='int drm_atomic_set_crtc_for_connector(struct drm_connector_state * , struct drm_crtc * )'/>
<use f='linux-4.14.y/drivers/gpu/drm/drm_atomic.c' l='1497' c='drm_atomic_set_crtc_for_connector'/>
<use f='linux-4.14.y/drivers/gpu/drm/drm_atomic.c' l='1497' u='a'/>
