<use f='linux-4.14.y/drivers/pci/pci-sysfs.c' l='1555' u='c'/>
<use f='linux-4.14.y/drivers/rtc/nvmem.c' l='63' u='c'/>
<use f='linux-4.14.y/kernel/module.c' l='1610' u='c'/>
<dec f='linux-4.14.y/include/linux/sysfs.h' l='184'/>
