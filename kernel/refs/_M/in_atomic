<use f='linux-4.14.y/kernel/exit.c' l='824' u='c'/>
<use f='linux-4.14.y/kernel/trace/trace_functions_graph.c' l='1406' u='c'/>
<use f='linux-4.14.y/kernel/workqueue.c' l='2123' u='c'/>
