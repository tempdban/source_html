<use f='linux-4.18.y/fs/ext4/inode.c' l='5238' u='c'/>
<use f='linux-4.18.y/fs/ext4/inode.c' l='5250' u='c'/>
<use f='linux-4.18.y/fs/ext4/inode.c' l='5604' u='c'/>
