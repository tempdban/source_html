<use f='linux-4.14.y/drivers/net/ethernet/intel/ixgbe/ixgbe_main.c' l='2150' u='c'/>
<use f='linux-4.14.y/drivers/net/ethernet/intel/ixgbe/ixgbe_main.c' l='3616' u='c'/>
<dec f='linux-4.14.y/drivers/net/ethernet/intel/ixgbe/ixgbe.h' l='155'/>
