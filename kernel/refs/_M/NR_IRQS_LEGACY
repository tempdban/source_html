<dec f='linux-4.14.y/arch/x86/include/asm/irq_vectors.h' l='133'/>
<use f='linux-4.14.y/include/linux/irq.h' l='512' u='a'/>
<doc f='linux-4.14.y/arch/x86/include/asm/irq_vectors.h' l='122'>/*
 * Size the maximum number of interrupts.
 *
 * If the irq_desc[] array has a sparse layout, we can size things
 * generously - it scales up linearly with the maximum number of CPUs,
 * and the maximum number of IO-APICs, whichever is higher.
 *
 * In other cases we size more conservatively, to not create too large
 * static arrays.
 */</doc>
<use f='linux-4.14.y/arch/x86/kernel/acpi/boot.c' l='111' u='c'/>
<use f='linux-4.14.y/arch/x86/kernel/acpi/boot.c' l='358' u='c'/>
<use f='linux-4.14.y/arch/x86/kernel/acpi/boot.c' l='491' u='c'/>
<use f='linux-4.14.y/arch/x86/kernel/apic/vector.c' l='37' u='c'/>
<use f='linux-4.14.y/arch/x86/kernel/apic/vector.c' l='410' u='c'/>
<use f='linux-4.14.y/arch/x86/kernel/i8259.c' l='409' u='c'/>
<use f='linux-4.14.y/kernel/softirq.c' l='765' u='c'/>
