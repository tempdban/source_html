<use f='linux-4.14.y/security/commoncap.c' l='1171' u='c'/>
<use f='linux-4.14.y/security/commoncap.c' l='1183' u='c'/>
<use f='linux-4.14.y/security/commoncap.c' l='1185' u='c'/>
