<doc f='linux-4.14.y/include/asm-generic/pgtable-nop4d.h' l='45'>/*
 * allocating and freeing a p4d is trivial: the 1-entry p4d is
 * inside the pgd, so has no extra memory associated with it.
 */</doc>
