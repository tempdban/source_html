<use f='linux-4.14.y/mm/slab.c' l='1330' u='w' c='kmem_cache_init_late'/>
<use f='linux-4.14.y/mm/slab.c' l='1358' u='w' c='cpucache_init'/>
<use f='linux-4.14.y/mm/slab.c' l='1843' u='r' c='setup_cpu_cache'/>
<use f='linux-4.14.y/mm/slab.c' l='1850' u='r' c='setup_cpu_cache'/>
<use f='linux-4.14.y/mm/slab.c' l='1853' u='r' c='setup_cpu_cache'/>
