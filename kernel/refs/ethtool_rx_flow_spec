<mbr r='ethtool_rx_flow_spec::h_ext' o='448' t='struct ethtool_flow_ext'/>
<mbr r='ethtool_rx_flow_spec::m_u' o='608' t='union ethtool_flow_union'/>
<mbr r='ethtool_rx_flow_spec::m_ext' o='1024' t='struct ethtool_flow_ext'/>
<mbr r='ethtool_rx_flow_spec::ring_cookie' o='1216' t='__u64'/>
<mbr r='ethtool_rx_flow_spec::location' o='1280' t='__u32'/>
