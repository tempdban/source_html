<doc f='linux-4.18.y/fs/crypto/fscrypt_private.h' l='23'>/**
 * Encryption context for inode
 *
 * Protector format:
 *  1 byte: Protector format (1 = this version)
 *  1 byte: File contents encryption mode
 *  1 byte: File names encryption mode
 *  1 byte: Flags
 *  8 bytes: Master Key descriptor
 *  16 bytes: Encryption Key derivation nonce
 */</doc>
<mbr r='fscrypt_context::format' o='0' t='u8'/>
<mbr r='fscrypt_context::contents_encryption_mode' o='8' t='u8'/>
<mbr r='fscrypt_context::filenames_encryption_mode' o='16' t='u8'/>
<mbr r='fscrypt_context::flags' o='24' t='u8'/>
<mbr r='fscrypt_context::master_key_descriptor' o='32' t='u8 [8]'/>
<mbr r='fscrypt_context::nonce' o='96' t='u8 [16]'/>
