<use f='linux-4.18.y/drivers/net/ixgbe/base/ixgbe_common.c' l='95' u='r' c='ixgbe_init_ops_generic'/>
<def f='linux-4.18.y/drivers/net/ixgbe/base/ixgbe_common.c' l='2441' ll='2500' type='s32 ixgbe_init_rx_addrs_generic(struct ixgbe_hw * hw)'/>
<doc f='linux-4.18.y/drivers/net/ixgbe/base/ixgbe_common.c' l='2433'>/**
 *  ixgbe_init_rx_addrs_generic - Initializes receive address filters.
 *  @hw: pointer to hardware structure
 *
 *  Places the MAC address in receive address register 0 and clears the rest
 *  of the receive address registers. Clears the multicast table. Assumes
 *  the receiver is in reset when the routine is called.
 **/</doc>
<dec f='linux-4.18.y/kernel/linux/kni/ethtool/ixgbe/ixgbe_common.h' l='58' type='s32 ixgbe_init_rx_addrs_generic(struct ixgbe_hw * hw)'/>
