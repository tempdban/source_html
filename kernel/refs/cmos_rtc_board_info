<mbr r='cmos_rtc_board_info::flags' o='128' t='u32'/>
<mbr r='cmos_rtc_board_info::address_space' o='160' t='int'/>
<mbr r='cmos_rtc_board_info::rtc_day_alarm' o='192' t='u8'/>
<mbr r='cmos_rtc_board_info::rtc_mon_alarm' o='200' t='u8'/>
<mbr r='cmos_rtc_board_info::rtc_century' o='208' t='u8'/>
