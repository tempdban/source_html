<mbr r='dpseci_congestion_notification_cfg::threshold_entry' o='32' t='uint32_t'/>
<mbr r='dpseci_congestion_notification_cfg::threshold_exit' o='64' t='uint32_t'/>
<mbr r='dpseci_congestion_notification_cfg::message_ctx' o='128' t='uint64_t'/>
<mbr r='dpseci_congestion_notification_cfg::message_iova' o='192' t='uint64_t'/>
<mbr r='dpseci_congestion_notification_cfg::dest_cfg' o='256' t='struct dpseci_dest_cfg'/>
<mbr r='dpseci_congestion_notification_cfg::notification_mode' o='352' t='uint16_t'/>
