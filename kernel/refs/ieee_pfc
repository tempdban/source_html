<mbr r='ieee_pfc::pfc_en' o='8' t='__u8'/>
<mbr r='ieee_pfc::mbc' o='16' t='__u8'/>
<mbr r='ieee_pfc::delay' o='32' t='__u16'/>
<mbr r='ieee_pfc::requests' o='64' t='__u64 [8]'/>
<mbr r='ieee_pfc::indications' o='576' t='__u64 [8]'/>
