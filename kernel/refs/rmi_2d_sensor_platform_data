<mbr r='rmi_2d_sensor_platform_data::y_mm' o='224' t='int'/>
<mbr r='rmi_2d_sensor_platform_data::disable_report_mask' o='256' t='int'/>
<mbr r='rmi_2d_sensor_platform_data::rezero_wait' o='288' t='u16'/>
<mbr r='rmi_2d_sensor_platform_data::topbuttonpad' o='304' t='bool'/>
<mbr r='rmi_2d_sensor_platform_data::kernel_tracking' o='312' t='bool'/>
<mbr r='rmi_2d_sensor_platform_data::dmax' o='320' t='int'/>
<mbr r='rmi_2d_sensor_platform_data::dribble' o='352' t='int'/>
