<dec f='linux-4.14.y/drivers/usb/core/hcd.c' l='2599' type='struct usb_hcd * usb_create_shared_hcd(const struct hc_driver * , struct device * , const char * , struct usb_hcd * )'/>
<use f='linux-4.14.y/drivers/usb/core/hcd.c' l='2599' c='usb_create_shared_hcd'/>
<use f='linux-4.14.y/drivers/usb/core/hcd.c' l='2599' u='a'/>
