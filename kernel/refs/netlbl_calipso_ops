<mbr r='netlbl_calipso_ops::sock_setattr' o='448' t='int (*)(struct sock *, const struct calipso_doi *, const struct netlbl_lsm_secattr *)'/>
<mbr r='netlbl_calipso_ops::sock_delattr' o='512' t='void (*)(struct sock *)'/>
