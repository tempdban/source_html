<use f='linux-4.14.y/drivers/usb/core/message.c' l='1023' c='usb_clear_halt'/>
<use f='linux-4.14.y/drivers/usb/core/message.c' l='1023' u='a'/>
<use f='linux-4.14.y/drivers/usb/core/message.c' l='1023' u='a'/>
