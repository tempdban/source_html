<use f='linux-4.14.y/fs/binfmt_elf.c' l='1496' u='c' c='fill_prstatus'/>
<use f='linux-4.14.y/kernel/acct.c' l='566' u='c' c='acct_collect'/>
<use f='linux-4.14.y/kernel/exit.c' l='141' u='c' c='__exit_signal'/>
