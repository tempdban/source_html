<doc f='linux-4.18.y/include/linux/writeback.h' l='45'>/*
 * A control structure which tells the writeback code what to do.  These are
 * always on the stack, and hence need no locking.  They are always initialised
 * in a manner such that unspecified fields are set to zero.
 */</doc>
<mbr r='writeback_control::nr_to_write' o='0' t='long'/>
<mbr r='writeback_control::pages_skipped' o='64' t='long'/>
<mbr r='writeback_control::range_start' o='128' t='loff_t'/>
<mbr r='writeback_control::range_end' o='192' t='loff_t'/>
<mbr r='writeback_control::sync_mode' o='256' t='enum writeback_sync_modes'/>
<mbr r='writeback_control::for_kupdate' o='288' t='unsigned int'/>
<mbr r='writeback_control::for_background' o='289' t='unsigned int'/>
<mbr r='writeback_control::tagged_writepages' o='290' t='unsigned int'/>
<mbr r='writeback_control::for_reclaim' o='291' t='unsigned int'/>
<mbr r='writeback_control::range_cyclic' o='292' t='unsigned int'/>
<mbr r='writeback_control::for_sync' o='293' t='unsigned int'/>
