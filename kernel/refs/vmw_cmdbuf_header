<mbr r='vmw_cmdbuf_header::cb_context' o='128' t='SVGACBContext'/>
<mbr r='vmw_cmdbuf_header::list' o='192' t='struct list_head'/>
<mbr r='vmw_cmdbuf_header::node' o='320' t='struct drm_mm_node'/>
<mbr r='vmw_cmdbuf_header::handle' o='1600' t='dma_addr_t'/>
<mbr r='vmw_cmdbuf_header::cmd' o='1664' t='u8 *'/>
<mbr r='vmw_cmdbuf_header::size' o='1728' t='size_t'/>
<mbr r='vmw_cmdbuf_header::reserved' o='1792' t='size_t'/>
<mbr r='vmw_cmdbuf_header::inline_space' o='1856' t='bool'/>
