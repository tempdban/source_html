<mbr r='sctp_remote_error::sre_error' o='64' t='__be16'/>
<mbr r='sctp_remote_error::sre_assoc_id' o='96' t='sctp_assoc_t'/>
<mbr r='sctp_remote_error::sre_data' o='128' t='__u8 [0]'/>
