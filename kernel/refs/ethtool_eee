<mbr r='ethtool_eee::advertised' o='64' t='__u32'/>
<mbr r='ethtool_eee::lp_advertised' o='96' t='__u32'/>
<mbr r='ethtool_eee::eee_active' o='128' t='__u32'/>
<mbr r='ethtool_eee::eee_enabled' o='160' t='__u32'/>
<mbr r='ethtool_eee::tx_lpi_enabled' o='192' t='__u32'/>
