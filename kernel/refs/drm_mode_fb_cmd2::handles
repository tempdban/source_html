<use f='linux-4.14.y/drivers/gpu/drm/drm_plane.c' l='752' u='w' c='drm_mode_cursor_universal'/>
<use f='linux-4.14.y/drivers/gpu/drm/qxl/qxl_display.c' l='1210' u='r' c='qxl_user_framebuffer_create'/>
<use f='linux-4.14.y/drivers/gpu/drm/virtio/virtgpu_display.c' l='319' u='r' c='virtio_gpu_user_framebuffer_create'/>
