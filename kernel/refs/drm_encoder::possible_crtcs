<use f='linux-4.14.y/drivers/gpu/drm/drm_fb_helper.c' l='2309' u='r' c='drm_pick_crtcs'/>
<use f='linux-4.14.y/drivers/gpu/drm/drm_simple_kms_helper.c' l='249' u='w' c='drm_simple_display_pipe_init'/>
<use f='linux-4.14.y/drivers/gpu/drm/qxl/qxl_display.c' l='1186' u='w' c='qdev_output_init'/>
