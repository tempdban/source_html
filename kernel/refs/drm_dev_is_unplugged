<use f='linux-4.14.y/drivers/gpu/drm/drm_gem.c' l='1006' u='c' c='drm_gem_mmap'/>
<use f='linux-4.14.y/drivers/gpu/drm/drm_ioctl.c' l='725' u='c' c='drm_ioctl_kernel'/>
<use f='linux-4.14.y/drivers/gpu/drm/drm_ioctl.c' l='774' u='c' c='drm_ioctl'/>
