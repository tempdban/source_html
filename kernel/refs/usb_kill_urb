<use f='linux-4.14.y/drivers/usb/core/urb.c' l='694' c='usb_kill_urb'/>
<use f='linux-4.14.y/drivers/usb/core/urb.c' l='694' u='a'/>
<use f='linux-4.14.y/drivers/usb/core/urb.c' l='694' u='a'/>
