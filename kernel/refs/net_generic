<def f='linux-4.18.y/include/net/netns/generic.h' l='39' ll='50' type='void * net_generic(const struct net * net, unsigned int id)'/>
<use f='linux-4.18.y/drivers/net/bonding/bond_main.c' l='4729' u='c' c='bond_init'/>
<use f='linux-4.18.y/drivers/net/bonding/bond_main.c' l='4807' u='c' c='bond_net_init'/>
