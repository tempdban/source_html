<use f='linux-4.18.y/arch/x86/kernel/fpu/regset.c' l='112' u='c' c='xstateregs_get'/>
<use f='linux-4.18.y/arch/x86/kernel/fpu/regset.c' l='313' u='c' c='fpregs_get'/>
<use f='linux-4.18.y/arch/x86/kernel/fpu/regset.c' l='326' u='c' c='fpregs_get'/>
<use f='linux-4.18.y/arch/x86/kernel/ptrace.c' l='745' u='c' c='ioperm_get'/>
