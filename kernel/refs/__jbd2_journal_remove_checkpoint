<dec f='linux-4.14.y/include/linux/jbd2.h' l='1275' type='int __jbd2_journal_remove_checkpoint(struct journal_head * )'/>
<use f='linux-4.14.y/fs/jbd2/checkpoint.c' l='100' u='c' c='__try_to_free_cp_buf'/>
