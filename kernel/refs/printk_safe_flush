<def f='linux-4.14.y/kernel/printk/printk_safe.c' l='259' ll='269' type='void printk_safe_flush()'/>
<use f='linux-4.14.y/kernel/printk/printk_safe.c' l='295' u='c' c='printk_safe_flush_on_panic'/>
