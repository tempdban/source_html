<def f='linux-4.14.y/drivers/scsi/megaraid/mbox_defs.h' l='759' ll='765'/>
<size>16</size>
<doc f='linux-4.14.y/drivers/scsi/megaraid/mbox_defs.h' l='750'>/**
 * struct private_bios_data - bios private data for boot devices
 * @geometry	: bits 0-3 - BIOS geometry, 0x0001 - 1GB, 0x0010 - 2GB,
 *		0x1000 - 8GB, Others values are invalid
 * @unused	: bits 4-7 are unused
 * @boot_drv	: logical drive set as boot drive, 0..7 - for 8LD cards,
 * 		0..39 - for 40LD cards
 * @cksum	: 0-(sum of first 13 bytes of this structure)
 */</doc>
<mbr r='private_bios_data::geometry' o='0' t='uint8_t'/>
<mbr r='private_bios_data::unused' o='4' t='uint8_t'/>
<mbr r='private_bios_data::boot_drv' o='8' t='uint8_t'/>
<mbr r='private_bios_data::rsvd' o='16' t='uint8_t [12]'/>
<mbr r='private_bios_data::cksum' o='112' t='uint16_t'/>
