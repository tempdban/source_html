<mbr r='drm_dp_mst_port::ddps' o='56' t='bool'/>
<mbr r='drm_dp_mst_port::pdt' o='64' t='u8'/>
<mbr r='drm_dp_mst_port::ldps' o='72' t='bool'/>
<mbr r='drm_dp_mst_port::dpcd_rev' o='80' t='u8'/>
