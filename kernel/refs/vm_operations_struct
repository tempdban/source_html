<mbr r='vm_operations_struct::huge_fault' o='320' t='int (*)(struct vm_fault *, enum page_entry_size)'/>
<mbr r='vm_operations_struct::map_pages' o='384' t='void (*)(struct vm_fault *, unsigned long, unsigned long)'/>
<mbr r='vm_operations_struct::page_mkwrite' o='448' t='int (*)(struct vm_fault *)'/>
