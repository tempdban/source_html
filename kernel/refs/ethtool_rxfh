<mbr r='ethtool_rxfh::key_size' o='96' t='__u32'/>
<mbr r='ethtool_rxfh::hfunc' o='128' t='__u8'/>
<mbr r='ethtool_rxfh::rsvd8' o='136' t='__u8 [3]'/>
