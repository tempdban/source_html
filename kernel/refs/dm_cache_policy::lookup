<use f='linux-4.14.y/drivers/md/dm-cache-policy-smq.c' l='1655' u='w' c='init_policy_functions'/>
<dec f='linux-4.14.y/drivers/md/dm-cache-policy.h' l='56' type='int (*)(struct dm_cache_policy *, dm_oblock_t, dm_cblock_t *, int, bool, bool *)'/>
<offset>64</offset>
