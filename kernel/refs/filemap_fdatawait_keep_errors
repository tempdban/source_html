<dec f='linux-4.14.y/include/linux/fs.h' l='2573' type='int filemap_fdatawait_keep_errors(struct address_space * mapping)'/>
<use f='linux-4.14.y/fs/fs-writeback.c' l='2277' u='c' c='wait_sb_inodes'/>
