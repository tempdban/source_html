<def f='linux-4.14.y/drivers/ptp/ptp_private.h' l='71' ll='75' type='int queue_cnt(struct timestamp_event_queue * q)'/>
<use f='linux-4.14.y/drivers/ptp/ptp_chardev.c' l='290' u='c' c='ptp_poll'/>
<use f='linux-4.14.y/drivers/ptp/ptp_chardev.c' l='317' u='c' c='ptp_read'/>
