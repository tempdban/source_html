<use f='linux-4.14.y/kernel/trace/trace.c' l='7710' u='c' c='instance_mkdir'/>
<use f='linux-4.14.y/kernel/trace/trace_events.c' l='1923' u='c' c='event_subsystem_dir'/>
<use f='linux-4.14.y/kernel/trace/trace_events.c' l='1982' u='c' c='event_create_dir'/>
<use f='linux-4.14.y/kernel/trace/trace_events.c' l='2919' u='c' c='create_event_toplevel_files'/>
<use f='linux-4.14.y/kernel/trace/trace_stat.c' l='283' u='c' c='tracing_stat_init'/>
