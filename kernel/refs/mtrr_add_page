<use f='linux-4.14.y/arch/x86/kernel/cpu/mtrr/if.c' l='301' u='c' c='mtrr_ioctl'/>
<def f='linux-4.14.y/arch/x86/kernel/cpu/mtrr/main.c' l='300' ll='405' type='int mtrr_add_page(unsigned long base, unsigned long size, unsigned int type, bool increment)'/>
<use f='linux-4.14.y/arch/x86/kernel/cpu/mtrr/main.c' l='460' u='c' c='mtrr_add'/>
<doc f='linux-4.14.y/arch/x86/kernel/cpu/mtrr/main.c' l='265'>/**
 * mtrr_add_page - Add a memory type region
 * @base: Physical base address of region in pages (in units of 4 kB!)
