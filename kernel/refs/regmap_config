<mbr r='regmap_config::reg_write' o='704' t='int (*)(void *, unsigned int, unsigned int)'/>
<mbr r='regmap_config::fast_io' o='768' t='bool'/>
<mbr r='regmap_config::max_register' o='800' t='unsigned int'/>
<mbr r='regmap_config::wr_table' o='832' t='const struct regmap_access_table *'/>
<mbr r='regmap_config::rd_table' o='896' t='const struct regmap_access_table *'/>
<mbr r='regmap_config::volatile_table' o='960' t='const struct regmap_access_table *'/>
<mbr r='regmap_config::precious_table' o='1024' t='const struct regmap_access_table *'/>
<mbr r='regmap_config::reg_defaults' o='1088' t='const struct reg_default *'/>
<mbr r='regmap_config::num_reg_defaults' o='1152' t='unsigned int'/>
<mbr r='regmap_config::cache_type' o='1184' t='enum regcache_type'/>
<mbr r='regmap_config::reg_defaults_raw' o='1216' t='const void *'/>
<mbr r='regmap_config::num_reg_defaults_raw' o='1280' t='unsigned int'/>
<mbr r='regmap_config::read_flag_mask' o='1344' t='unsigned long'/>
<mbr r='regmap_config::write_flag_mask' o='1408' t='unsigned long'/>
<mbr r='regmap_config::use_single_rw' o='1472' t='bool'/>
<mbr r='regmap_config::can_multi_write' o='1480' t='bool'/>
<mbr r='regmap_config::reg_format_endian' o='1504' t='enum regmap_endian'/>
<mbr r='regmap_config::val_format_endian' o='1536' t='enum regmap_endian'/>
<mbr r='regmap_config::ranges' o='1600' t='const struct regmap_range_cfg *'/>
<mbr r='regmap_config::num_ranges' o='1664' t='unsigned int'/>
