<mbr r='datapath::list_node' o='128' t='struct list_head'/>
<mbr r='datapath::table' o='256' t='struct flow_table'/>
<mbr r='datapath::ports' o='640' t='struct hlist_head *'/>
<mbr r='datapath::stats_percpu' o='704' t='struct dp_stats_percpu *'/>
<mbr r='datapath::net' o='768' t='possible_net_t'/>
<mbr r='datapath::user_features' o='832' t='u32'/>
<mbr r='datapath::max_headroom' o='864' t='u32'/>
