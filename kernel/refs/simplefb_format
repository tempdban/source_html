<size>64</size>
<doc f='linux-4.14.y/include/linux/platform_data/simplefb.h' l='33'>/*
 * Data-Format for Simple-Framebuffers
 * @name: unique 0-terminated name that can be used to identify the mode
 * @red,green,blue: Offsets and sizes of the single RGB parts
 * @transp: Offset and size of the alpha bits. length=0 means no alpha
 * @fourcc: 32bit DRM four-CC code (see drm_fourcc.h)
 */</doc>
<mbr r='simplefb_format::name' o='0' t='const char *'/>
<mbr r='simplefb_format::bits_per_pixel' o='64' t='u32'/>
<mbr r='simplefb_format::red' o='96' t='struct fb_bitfield'/>
<mbr r='simplefb_format::green' o='192' t='struct fb_bitfield'/>
<mbr r='simplefb_format::blue' o='288' t='struct fb_bitfield'/>
<mbr r='simplefb_format::transp' o='384' t='struct fb_bitfield'/>
<mbr r='simplefb_format::fourcc' o='480' t='u32'/>
