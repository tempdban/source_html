<def f='linux-4.14.y/drivers/acpi/acpica/nsalloc.c' l='106' ll='150' type='void acpi_ns_delete_node(struct acpi_namespace_node * node)'/>
<use f='linux-4.14.y/drivers/acpi/acpica/nsalloc.c' l='199' u='c' c='acpi_ns_remove_node'/>
