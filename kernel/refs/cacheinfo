<def f='linux-4.14.y/include/linux/cacheinfo.h' l='48' ll='72'/>
<size>72</size>
<doc f='linux-4.14.y/include/linux/cacheinfo.h' l='20'>/**
 * struct cacheinfo - represent a cache leaf node
 * @id: This cache&apos;s id. It is unique among caches with the same (type, level).
 * @type: type of the cache - data, inst or unified
 * @level: represents the hierarchy in the multi-level cache
 * @coherency_line_size: size of each cache line usually representing
 *	the minimum amount of data that gets transferred from memory
 * @number_of_sets: total number of sets, a set is a collection of cache
 *	lines sharing the same index
 * @ways_of_associativity: number of ways in which a particular memory
 *	block can be placed in the cache
 * @physical_line_partition: number of physical cache lines sharing the
 *	same cachetag
 * @size: Total size of the cache
 * @shared_cpu_map: logical cpumask representing all the cpus sharing
 *	this cache node
 * @attributes: bitfield representing various cache attributes
 * @of_node: if devicetree is used, this represents either the cpu node in
 *	case there&apos;s no explicit cache node or the cache node itself in the
 *	device tree
 * @disable_sysfs: indicates whether this node is visible to the user via
 *	sysfs or not
 * @priv: pointer to any private data structure specific to particular
 *	cache design
 *
 * While @of_node, @disable_sysfs and @priv are used for internal book
 * keeping, the remaining members form the core properties of the cache
 */</doc>
<mbr r='cacheinfo::id' o='0' t='unsigned int'/>
<mbr r='cacheinfo::type' o='32' t='enum cache_type'/>
<mbr r='cacheinfo::level' o='64' t='unsigned int'/>
<mbr r='cacheinfo::coherency_line_size' o='96' t='unsigned int'/>
<mbr r='cacheinfo::number_of_sets' o='128' t='unsigned int'/>
<mbr r='cacheinfo::ways_of_associativity' o='160' t='unsigned int'/>
<mbr r='cacheinfo::physical_line_partition' o='192' t='unsigned int'/>
<mbr r='cacheinfo::size' o='224' t='unsigned int'/>
<mbr r='cacheinfo::shared_cpu_map' o='256' t='cpumask_t'/>
<mbr r='cacheinfo::attributes' o='320' t='unsigned int'/>
<mbr r='cacheinfo::of_node' o='384' t='struct device_node *'/>
<mbr r='cacheinfo::disable_sysfs' o='448' t='bool'/>
<mbr r='cacheinfo::priv' o='512' t='void *'/>
<def f='linux-4.14.y/include/linux/cacheinfo.h' l='48' ll='72'/>
