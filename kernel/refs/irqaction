<mbr r='irqaction::percpu_dev_id' o='128' t='void *'/>
<mbr r='irqaction::next' o='192' t='struct irqaction *'/>
<mbr r='irqaction::thread_fn' o='256' t='irq_handler_t'/>
<mbr r='irqaction::thread' o='320' t='struct task_struct *'/>
<mbr r='irqaction::secondary' o='384' t='struct irqaction *'/>
<mbr r='irqaction::irq' o='448' t='unsigned int'/>
<mbr r='irqaction::flags' o='480' t='unsigned int'/>
<mbr r='irqaction::thread_flags' o='512' t='unsigned long'/>
<mbr r='irqaction::thread_mask' o='576' t='unsigned long'/>
