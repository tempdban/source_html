<mbr r='drm_mode_set::mode' o='128' t='struct drm_display_mode *'/>
<mbr r='drm_mode_set::x' o='192' t='uint32_t'/>
<mbr r='drm_mode_set::y' o='224' t='uint32_t'/>
<mbr r='drm_mode_set::connectors' o='256' t='struct drm_connector **'/>
<mbr r='drm_mode_set::num_connectors' o='320' t='size_t'/>
