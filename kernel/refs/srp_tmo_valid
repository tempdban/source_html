<def f='linux-4.14.y/drivers/scsi/scsi_transport_srp.c' l='98' ll='115' type='int srp_tmo_valid(int reconnect_delay, int fast_io_fail_tmo, long dev_loss_tmo)'/>
<dec f='linux-4.14.y/drivers/scsi/scsi_transport_srp.c' l='116' type='int srp_tmo_valid(int , int , long )'/>
<use f='linux-4.14.y/drivers/scsi/scsi_transport_srp.c' l='116' c='srp_tmo_valid'/>
<use f='linux-4.14.y/drivers/scsi/scsi_transport_srp.c' l='116' u='a'/>
