<mbr r='drm_crtc_funcs::page_flip_target' o='512' t='int (*)(struct drm_crtc *, struct drm_framebuffer *, struct drm_pending_vblank_event *, uint32_t, uint32_t, struct drm_modeset_acquire_ctx *)'/>
