<mbr r='display_timing::hfront_porch' o='192' t='struct timing_entry'/>
<mbr r='display_timing::hback_porch' o='288' t='struct timing_entry'/>
<mbr r='display_timing::hsync_len' o='384' t='struct timing_entry'/>
<mbr r='display_timing::vactive' o='480' t='struct timing_entry'/>
<mbr r='display_timing::vfront_porch' o='576' t='struct timing_entry'/>
<mbr r='display_timing::vback_porch' o='672' t='struct timing_entry'/>
