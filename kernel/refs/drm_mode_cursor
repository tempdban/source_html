<size>28</size>
<doc f='linux-4.14.y/include/uapi/drm/drm_mode.h' l='530'>/*
 * depending on the value in flags different members are used.
 *
 * CURSOR_BO uses
 *    crtc_id
 *    width
 *    height
 *    handle - if 0 turns the cursor off
 *
 * CURSOR_MOVE uses
 *    crtc_id
 *    x
 *    y
 */</doc>
<mbr r='drm_mode_cursor::flags' o='0' t='__u32'/>
<mbr r='drm_mode_cursor::crtc_id' o='32' t='__u32'/>
<mbr r='drm_mode_cursor::x' o='64' t='__s32'/>
<mbr r='drm_mode_cursor::y' o='96' t='__s32'/>
<mbr r='drm_mode_cursor::width' o='128' t='__u32'/>
<mbr r='drm_mode_cursor::height' o='160' t='__u32'/>
<mbr r='drm_mode_cursor::handle' o='192' t='__u32'/>
