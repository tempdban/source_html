<mbr r='ipsec_encap_pdb::seq_num_ext_hi' o='32' t='uint32_t'/>
<mbr r='ipsec_encap_pdb::seq_num' o='64' t='uint32_t'/>
<mbr r='ipsec_encap_pdb::spi' o='256' t='uint32_t'/>
<mbr r='ipsec_encap_pdb::ip_hdr_len' o='288' t='uint32_t'/>
<mbr r='ipsec_encap_pdb::ip_hdr' o='320' t='uint8_t [0]'/>
