<mbr r='ib_t10_dif_domain::ref_tag' o='96' t='u32'/>
<mbr r='ib_t10_dif_domain::ref_remap' o='128' t='bool'/>
<mbr r='ib_t10_dif_domain::app_escape' o='136' t='bool'/>
<mbr r='ib_t10_dif_domain::ref_escape' o='144' t='bool'/>
