<mbr r='mpt3_ioctl_iocinfo::hw_rev' o='192' t='uint32_t'/>
<mbr r='mpt3_ioctl_iocinfo::subsystem_device' o='224' t='uint32_t'/>
<mbr r='mpt3_ioctl_iocinfo::subsystem_vendor' o='256' t='uint32_t'/>
