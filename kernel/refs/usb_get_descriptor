<dec f='linux-4.14.y/drivers/usb/core/message.c' l='659' type='int usb_get_descriptor(struct usb_device * , unsigned char , unsigned char , void * , int )'/>
<use f='linux-4.14.y/drivers/usb/core/message.c' l='659' c='usb_get_descriptor'/>
