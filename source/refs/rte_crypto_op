<mbr r='rte_crypto_op::sess_type' o='16' t='uint8_t'/>
<mbr r='rte_crypto_op::private_data_offset' o='32' t='uint16_t'/>
<mbr r='rte_crypto_op::reserved' o='48' t='uint8_t [3]'/>
<mbr r='rte_crypto_op::mempool' o='128' t='struct rte_mempool *'/>
<mbr r='rte_crypto_op::phys_addr' o='192' t='rte_iova_t'/>
