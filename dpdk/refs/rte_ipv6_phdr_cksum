<use f='master/lib/librte_net/rte_net.h' l='162' u='c' c='rte_net_intel_cksum_flags_prepare'/>
<use f='master/drivers/net/tap/rte_eth_tap.c' l='485' u='c' c='tap_tx_l3_cksum'/>
